$(document).ready(function(){
  console.log('ready');
  init();
});

var public_spreadsheet_url = 'https://docs.google.com/spreadsheet/pub?hl=en_US&hl=en_US&key=12TI3Btn9z5uzsQxWsUZ26fwyYoY2WYsl55xdDJJlwTI&output=html';
var cal = ics();

function init() {
  Tabletop.init( 
    { key: public_spreadsheet_url,
     callback: showInfo,
     simpleSheet: true }
  );
  
}


function alertthis(){
  cal.download();
}


function showInfo(data, tabletop) { 
 // initMap(data);
    
  if ($('#tourmap').length > 0)
    initMap(data);
  
  var html = "";
  if ($('#sts9-tours').length > 0){
    $.each(data,function(){
      
      var dateStr = this['Begin'];
      if (this['End'] != "")
        dateStr += " - "+this['End'];
     
      var evEnd = this['End'] || this['Begin'];
      cal.addEvent('STS9 at '+this.Venue+' in '+this.City , 'Tickets: '+this.Tickets, this.Venue+', '+this.City, this.Begin, evEnd);
      
      for (var property in this) {
        if (this.hasOwnProperty(property)) {

          if (this.property == 'undefined'){
            this[property] = "";
          }else{

            if ((property == 'Tickets')&&(this.property != "")){
              ht = "<a target='_blank' class='btn' href='"+this[property]+"'>Tickets</a>";
              this[property] = ht;
            }
            
            if ((property == 'VIP')&&(this.property != "")){
              ht = (this[property]=="" ? "" : "<a target='_blank' class='btn' href='"+this[property]+"'>VIP</a>");
              this[property] = ht;
            }

             if ((property == 'Hotels')&&(this.property != "")){
              ht = (this[property]=="" ? "" : "<a target='_blank' class='btn' href='"+this[property]+"'>Travel</a>");
              this[property] = ht;
            }

             if ((property == 'RSVP')&&(this.property != "")){
              ht = "<a target='_blank' class='btn' href='"+this[property]+"'>RSVP</a>";
              this[property] = ht;
            }
          }

        }
      }
      
      html += "<tr><td class='date'>"+dateStr+"</td><td class='venue'>"+this.Venue+"</td><td class='city'>"+this.City+"</td><td class='tickets'>"+this.Tickets+"</td><td class='vip'>"+this.VIP+"</td><td class='hotels'>"+this.Hotels+"</td><td class='rsvp'>"+this.RSVP+"</tr>";
    });
    $('#sts9-tours').append(html);
    
    
  }  
}



